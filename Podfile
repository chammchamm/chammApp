  pod 'Alamofire', '~> 5.0.0-beta.5'

  target 'chammAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'chammAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
